generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")  // .env 파일에 MySQL 접속 주소 필요
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  name      String?
  createdAt DateTime @default(now())
}
odel Log {
  id        Int      @id @default(autoincrement())
  userId    Int?
  action    String
  detail    String?
  createdAt DateTime @default(now())
}